{
  "cleanUrls": true,
  "rewrites": [
    { 
      "source": "/:path((?!.*\\.).*)", 
      "destination": "/:path.html" 
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "all"
        }
      ]
    },
    {
      "source": "/faviconlogo.png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "Content-Type",
          "value": "image/png"
        }
      ]
    }
  ]
}
